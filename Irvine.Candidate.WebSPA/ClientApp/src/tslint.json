{
    "extends": "../tslint.json",
    "rules": {
        "use-host-property-decorator": false,
        "no-input-rename": false,
        "no-output-on-prefix": false,
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}